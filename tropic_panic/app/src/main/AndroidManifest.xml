<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android_attrape_objet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Android_attrape_objet">

        <activity android:name=".view.ActivitePrincipale" android:theme="@style/Theme.Android_attrape_objet.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".view.ActiviteJouer" android:screenOrientation="portrait" android:theme="@style/Theme.Android_attrape_objet.NoActionBar"/>
        <activity android:name=".view.ActiviteScores" android:theme="@style/Theme.Android_attrape_objet.NoActionBar"/>
    </application>

</manifest>